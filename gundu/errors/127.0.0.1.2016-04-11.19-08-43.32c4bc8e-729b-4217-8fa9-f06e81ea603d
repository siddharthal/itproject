ger autoincrement\' field\n## Consult manual for more options, validators, etc.\n##\n## More API examples for controllers:\n##\n## >>> db.mytable.insert(myfield=\'value\')\n## >>> rows=db(db.mytable.myfield==\'value\').select(db.mytable.ALL)\n## >>> for row in rows: print row.id, row.myfield\n#########################################################################\n\n## after defining tables, uncomment below to enable auditing\n# auth.enable_record_versioning(db)\ndb.define_table("teams",\nField("name",requires=IS_NOT_EMPTY()),\nField("moderator","reference auth_user.id",readable=False,writable=False),\nauth.signature\n)\ndb.define_table("agenda",\nField("title",requires=IS_NOT_EMPTY()),\nField("DT","datetime",readable=False,writable=False),\nField("venue",requires=IS_NOT_EMPTY()),\nauth.signature\n)\ndb.define_table("tble",\nField("teams_id",readable=False,writable=False),\nField("auth_user_id","reference auth_user",default=auth.user_id),\n)\ndb.define_table("topic",\nField("agenda_id","reference agenda"),\nField("title","string",requires=IS_NOT_EMPTY()),\nField("description","text",requires=IS_NOT_EMPTY()),\n)\ndb.define_table("minutes",\n                Field("Reference","string",requires=IS_NOT_EMPTY()),\n                Field("agenda_id","reference agenda"),\n                Field("teams_id","reference teams"),\n)\n'
